<script>
    var QueryString = enso_query_string();
    var pagination = document.getElementById("page-nav");
    // class="on"
    var thisPage = QueryString.page ? QueryString.page : 1;  // depends on common.js
    var pages = pagination.querySelectorAll("a");
    var prev = pagination.querySelector(".prev");
    var next = pagination.querySelector(".next");
    var q = QueryString.q;
    var MAX_PAGES = 10;

    for (var i = 0; i < pages.length; i++) {
        var n = parseInt(pages[i].innerText);  // 页码
        if (pages[i].classList.contains("prev") || pages[i].classList.contains("next"))
            continue;

        if (thisPage == n) {
            pages[i].classList.contains("on") || pages[i].classList.add("on");
            pages[i].href = 'javascript:;';
        } else {
            pages[i].classList.remove("on");
            pages[i].href = '?q='+q + '&page=' + n;
        }
        if (thisPage == 1) {
            prev.style.display = 'none';
            next.style.display = 'inline-block';
            prev.href = '?q='+q + '&page=' + 1;
            next.href = '?q='+q + '&page=' + (parseInt(thisPage)+1);
        } else if (thisPage == MAX_PAGES) {
            prev.style.display = 'inline-block';
            next.style.display = 'none';
            prev.href = '?q='+q + '&page=' + (thisPage-1);
            next.href = '?q='+q + '&page=' + MAX_PAGES;
        } else {
            prev.style.display = 'inline-block';
            next.style.display = 'inline-block';
            prev.href = '?q='+q + '&page=' + (thisPage-1);
            next.href = '?q='+q + '&page=' + (parseInt(thisPage)+1);
        }
    }
</script>
